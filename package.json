{
  "name": "ma-mft",
  "version": "1.0.0",
  "description": "Ma Peiqi MFT Associate Website - Hugo + TinaCMS",
  "main": "index.js",
  "scripts": {
    "dev": "hugo server & tinacms dev",
    "start": "npm run dev",
    "build": "tinacms build && hugo --minify --destination public",
    "hugo-only": "hugo --minify --destination public",
    "clean": "rm -rf public/ && rm -rf .hugo_build.lock",
    "stop": "pkill -f hugo && pkill -f tinacms",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": ["hugo", "therapy", "mft", "tinacms"],
  "author": "Peiqi Ma",
  "license": "ISC",
  "dependencies": {
    "tinacms": "^2.7.9"
  },
  "devDependencies": {
    "@tinacms/cli": "^1.9.9",
    "@types/node": "^24.0.7"
  }
}
